utton>\n              </form>\n              <h4>\n                Don't have an account? \n                <Link className=\"text-link no-underline\" to=\"/register\"> Sign up</Link>\n              </h4>\n              {error && (\n                <div className=\"my-3 p-3 bg-danger text-white\">\n                  {error.message}\n                </div>\n              )}\n            </div>\n\n\n        </div>\n  );\n}\n\nexport default Login;\n","/Users/nicoletrieu/Documents/GitHub/meet-n-treats/client/src/components/Sidebar/Sidebar.js",[],"/Users/nicoletrieu/Documents/GitHub/meet-n-treats/client/src/pages/Create/Create.js",["196","197"],"/Users/nicoletrieu/Documents/GitHub/meet-n-treats/client/src/pages/Home/Home.js",["198","199","200","201"],"import { useQuery } from \"@apollo/client\";\nimport Login from \"../../components/Login\";\n\nimport { QUERY_ME } from \"../../utils/queries\";\nimport { QUERY_POSTS } from \"../../utils/queries\";\n\nimport { Link } from \"react-router-dom\";\n\nimport PostCard from \"../../components/PostCard/PostCard\";\n\nfunction Home(props) {\n  const { posts } = props;\n\n  // const { loading, data } = useQuery(QUERY_ME);\n  // const me = data?.me || []; // *QUESTION: Why set to empty array instead of empty object? To avoid null or undefined references\n\n  // if (me.length === 0) {\n  //   return (\n  //     <Login />\n  //   )\n  // }\n\n  return (\n    <div className=\"flex-column align-center max-100-vh\">\n      {posts && \n        posts.map((post) => (\n          <div key={post._id} className='app-postcard_page ml-7 my-5 px-6 py-5'>\n            <PostCard \n              postsData={post}\n            />\n            <Link className='' to={`/posts/${post._id}`} style={{ textDecoration: 'none' }}>\n              <p className='light-text'>View post and leave comments</p>\n            </Link>\n          </div>\n      ) || (\n        <div>\n          <h2>No posts yet. Add your own post with \"📝 Create\"!</h2>\n        </div>\n      ))}\n    </div>\n  );\n}\n\nexport default Home;\n","/Users/nicoletrieu/Documents/GitHub/meet-n-treats/client/src/pages/Explore/Explore.js",["202"],"import React from \"react\";\nimport { useQuery } from \"@apollo/client\";\n\nfunction ExplorePage() {\n    return(\n      <div className=\"page explore\">\n            <div className=\"mb-1\">\n                <p>Discover new pets and stay updated on your favorites</p>\n            </div>\n            <div className=\"exSection\">\n                <h2>Popular Pets</h2>\n                <ul>\n                    <li>\n                        <img src=\"https://upload.wikimedia.org/wikipedia/commons/9/90/Labrador_Retriever_portrait.jpg\" alt=\"Pet 1\" />\n                        <h3>Lucky</h3>\n                        <p>Loves to play fetch and nap</p>\n                    </li>\n                    <li>\n                        <img src=\"https://www.readersdigest.ca/wp-content/uploads/2013/02/beagle-most-popular-dog-breeds.jpg\" alt=\"Pet 2\" />\n                        <h3>Brodie</h3>\n                        <p>Enjoys long walks</p>\n                    </li>\n                    <li>\n                        <img src=\"https://bestlifeonline.com/wp-content/uploads/sites/3/2019/03/shiba-inu.jpg?resize=500,333&quality=82&strip=all\" alt=\"Pet 3\" />\n                        <h3>Spike</h3>\n                        <p>Loves to cuddle</p>\n                    </li>\n                </ul>\n                {/* <button>See More Popular Pets</button> */}\n            </div>\n            <div className=\"exSection\">\n                <h2>Recommend Pets</h2>\n                <ul>\n                    <li>\n                        <img src=\"https://pyxis.nymag.com/v1/imgs/6a9/2a3/6c37ee37a2473f12b6dc4a703a50381993-09-clifford.jpg\" alt=\"Pet 4\" />\n                        <h3>Clifford</h3>\n                        <p>The big red dog</p>\n                    </li>\n                    <li>\n                        <img src=\"https://upload.wikimedia.org/wikipedia/en/thumb/b/bc/Garfield_the_Cat.svg/800px-Garfield_the_Cat.svg.png\" alt=\"Pet 5\" />\n                        <h3>Garfield</h3>\n                        <p>Loves lasagna</p>\n                    </li>\n                    <li>\n                        <img src=\"https://upload.wikimedia.org/wikipedia/en/f/f6/Tom_Tom_and_Jerry.png\" alt=\"Pet 6\" />\n                        <h3>Tom</h3>\n                        <p>Excellent at catching mouse</p>\n                    </li>\n                </ul>\n                {/* <button>See More Recommended Pets</button> */}\n            </div>\n        </div>\n    );\n}\n\nexport default ExplorePage;","/Users/nicoletrieu/Documents/GitHub/meet-n-treats/client/src/pages/PlayDate/PlayDate.js",["203","204","205"],"import React, { useState } from \"react\";\n\nimport Login from \"../../components/Login\";\nimport MyCalendar from \"../../components/Calendar\";\n\nfunction PlayDatePage() {\n    const [pet1, setPet1] = useState(\"\");\n    const [pet2, setPet2] = useState(\"\");\n    const [location, setLocation] = useState(\"\");\n    const [activity, setActivity] = useState(\"\");\n    const [selectedDate, setSelectedDate] = useState(\"\");\n    const [playDates, setPlayDates] = useState(\"\");\n    const [showCalendar, setShowCalendar] = useState(false);\n\n    const handleSubmit = (event) => {\n        event.preventDefault();\n        //TODO: add logic or API cal to schedule play date\n        console.log(`Scheduled play date between ${pet1} and ${pet2} at ${location} for ${activity}`);\n        // setPlayDates([...playDates, newPlayDate]);\n    };\n\n    return (\n        <div className=\"page\">\n          <div className=\"playdate-form-card\">\n            <h2>Schedule a Play Date</h2>\n            <form onSubmit={handleSubmit}>\n              <h2>Pet 2</h2>\n              <input\n                type=\"text\"\n                value={pet2}\n                onChange={(e) => setPet2(e.target.value)}\n                placeholder=\"Enter pet name or username\"\n              />\n              <h2>Location</h2>\n              <input\n                type=\"text\"\n                value={location}\n                onChange={(e) => setLocation(e.target.value)}\n                placeholder=\"Enter a location\"\n              />\n              <h2>Activity</h2>\n              <input \n                type=\"text\"\n                id=\"activity\"\n                value={activity}\n                onChange={(e) => setActivity(e.target.value)}\n                placeholder=\"Enter an activity for a play date\"\n              />\n              <h2>Date</h2>\n              <input \n                type=\"text\"\n                id=\"date\"\n                value={selectedDate ? selectedDate.toDateString() : ''}\n                onClick={() => setShowCalendar(true)}\n                // placeholder=\"Click to select a date\"\n              />\n              {showCalendar && \n                <MyCalendar \n                  value={new Date()}\n                  style={{\n                    width: \"500px\",\n                    height: \"500px\",\n                    margin: \"0 auto\",\n                    backgroundColor: \"#f2f2f2\",\n                    borderRadius: \"10px\"\n                  }}\n                  selectedDate={selectedDate}\n                  setSelectedDate={setSelectedDate}\n                  setShowCalendar={setShowCalendar}\n                />\n              }\n              <button type=\"submit\">Schedule Play Date</button>\n            </form>  \n          </div>\n          <div className=\"calendar-card\">\n            <MyCalendar playDates={playDates} />\n          </div>\n        </div>\n    );\n}\n\nexport default PlayDatePage;","/Users/nicoletrieu/Documents/GitHub/meet-n-treats/client/src/pages/Profile/Profile.js",["206","207","208","209"],"/Users/nicoletrieu/Documents/GitHub/meet-n-treats/client/src/pages/Search/Search.js",["210","211"],"/Users/nicoletrieu/Documents/GitHub/meet-n-treats/client/src/pages/Market/MarketHome.js",["212","213","214","215"],"import { Link } from \"react-router-dom\";\nimport MarketBuy from \"../MarketBuy\";\nimport MarketSell from \"../MarketSell\";\n\nfunction MarketHome() {\n  const buyLink = <Link\n                to={`/marketplace/buy`}\n                style={{ textDecoration: 'none' }}>\n                <p className='light-text'>Buy</p>\n              </Link>\n  const sellLink = <Link\n                to={`/marketplace/sell`}\n                style={{ textDecoration: 'none' }}>\n                <p className='light-text'>Sell</p>\n              </Link>\n  return(\n    <div className=\"market-home page pet-info-card\">\n      <h1 className=\"market-home__greeting\">Welcome to the Marketplace</h1>\n      <div className=\"market-home__options\">\n        <Link to={`/marketplace/buy`} className=\"market-home__option market-home__option--buy\">\n          <h2>Buy</h2>\n        </Link>\n        <Link to={`/marketplace/sell`} className=\"market-home__option market-home__option--sell\">\n          <h2>Sell</h2>\n        </Link>\n      </div>\n    </div>\n  )\n}\n\nexport default MarketHome;","/Users/nicoletrieu/Documents/GitHub/meet-n-treats/client/src/pages/MarketSell/MarketSell.js",["216","217","218","219"],"import React, { useState } from \"react\";\nimport { useQuery, useMutation } from \"@apollo/client\";\n\nimport { QUERY_ITEMS_TO_SELL } from \"../../utils/queries\";\nimport { CREATE_ITEM_TO_SELL, REMOVE_ITEM } from \"../../utils/mutations\";\n\nimport { Link } from \"react-router-dom\";\n\nimport PostCard from \"../../components/PostCard/PostCard\";\n\nimport CloudinaryUploadWidget from \"../../components/Cloudinary/UploadWidget\";\nimport Auth from \"../../utils/auth\";\n\nfunction MarketSell() {\n  const [itemImageURL, setItemImageURL] = useState(\"\");\n  const [itemTitle, setItemTitle] = useState(\"\");\n  const [itemDescription, setItemDescription] = useState(\"\");\n  const [itemPrice, setItemPrice] = useState(0.0);\n\n  const { loadingItems, data: itemData } = useQuery(QUERY_ITEMS_TO_SELL, {\n    variables: {\n      ownerID: Auth.getUser().data._id,\n    },\n  });\n\n  let items = itemData?.items_to_sell;\n\n  const [CreateItem] = useMutation(CREATE_ITEM_TO_SELL, {\n    refetchQueries: [{ query: QUERY_ITEMS_TO_SELL }, \"items_to_sell\"],\n  });\n  const [RemoveItem] = useMutation(REMOVE_ITEM, {\n    refetchQueries: [{ query: QUERY_ITEMS_TO_SELL }, \"items_to_sell\"],\n  });\n\n  const handleCreateItem = (e) => {\n    e.preventDefault();\n    CreateItem({\n      variables: {\n        owner: Auth.getUser().data._id,\n        title: itemTitle,\n        description: itemDescription,\n        price: itemPrice,\n        photo: itemImageURL,\n      },\n      onCompleted: () => {\n        console.log(\"completed\");\n      },\n      onError: (error) => {\n        console.log(JSON.stringify(error, 2, null));\n      },\n    });\n  };\n\n  const handleRemove = (e, itemID) => {\n    e.preventDefault();\n    RemoveItem({\n      variables: {\n        itemID,\n      },\n      onCompleted: () => {\n        console.log(\"completed\");\n      },\n      onError: (error) => {\n        console.log(JSON.stringify(error, 2, null));\n      },\n    });\n  };\n\n  if (items == null) {\n    items = [];\n  }\n\n  return (\n    <div className=\"page pet-form-card flex-column align-center\">\n      <h2>📈 Make Your Sale</h2>\n      <form>\n        <input\n          name=\"itemTitle\"\n          placeholder=\"Enter item title\"\n          values={itemTitle}\n          onChange={(e) => setItemTitle(e.target.value)}\n          type=\"text\"\n        />\n        <textarea\n          name=\"itemDescription\"\n          placeholder=\"Enter item description\"\n          value={itemDescription}\n          onChange={(e) => setItemDescription(e.target.value)}\n        />\n        <input\n          name=\"itemPrice\"\n          placeholder=\"Enter price\"\n          values={itemPrice}\n          onChange={(e) => setItemPrice(e.target.value)}\n          type=\"number\"\n        />\n        <br />\n        <CloudinaryUploadWidget setPostImageURL={setItemImageURL} />\n        <br />\n        <button onClick={(e) => handleCreateItem(e)} className=\"btn btn-link\">Submit</button>\n      </form>\n      <hr></hr>\n      <h4>Check out items you are selling:</h4>\n      <p>Nothing yet...</p>\n      {items.map((item) => {\n        let price = item?.itemPrice;\n        if (price != null && !price.includes(\".\")) {\n          price = \"$\" + price + \".00\";\n        }\n        return (\n          <div key={item._id}>\n            <h4>{item.itemTitle}</h4>\n            <button onClick={(e) => handleRemove(e, item._id)}>Remove</button>\n            <p>{price}</p>\n            {/* Render uploaded image from Cloudinary: */}\n            <img id=\"uploadedimage\" src={item.itemPhoto} width={500}></img>\n            {/* Render author's name: */}\n            <p>\n              {item.itemDescription}\n              <br />\n              <span className=\"username\">STATUS: {item.itemStatus}</span>\n            </p>\n            <hr />\n          </div>\n        );\n      })}\n    </div>\n  );\n}\n\nexport default MarketSell;","/Users/nicoletrieu/Documents/GitHub/meet-n-treats/client/src/pages/MarketBuy/MarketBuy.js",["220","221","222","223","224"],"import { useQuery, useMutation } from \"@apollo/client\";\nimport Login from \"../../components/Login\";\n\nimport { QUERY_ITEMS_TO_BUY } from \"../../utils/queries\";\nimport { PURCHASE_ITEM } from \"../../utils/mutations\";\n\nimport { Link } from \"react-router-dom\";\n\nimport React from \"react\";\n\nimport PostCard from \"../../components/PostCard/PostCard\";\n\nimport Auth from \"../../utils/auth\";\n\nfunction MarketBuy() {\n  const { loadingItems, error, data } = useQuery(QUERY_ITEMS_TO_BUY, {\n    variables: {\n      ownerID: Auth.getUser().data._id,\n    },\n  });\n  const [Purch